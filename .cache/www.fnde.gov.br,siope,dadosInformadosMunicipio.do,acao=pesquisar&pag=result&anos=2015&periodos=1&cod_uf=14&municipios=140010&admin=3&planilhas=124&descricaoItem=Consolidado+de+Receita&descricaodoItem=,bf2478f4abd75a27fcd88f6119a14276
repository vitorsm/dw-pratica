        var load_time;
        if (typeof(pkPageLoadStart) != 'undefined' && pkPageLoadStart != null){
            load_time = page_load_end.getTime() - pkPageLoadStart.getTime();
        } else {
            load_time=-1;
        }
        _paq.push(["setTrackerUrl", pkBaseURL + "piwik.php"]);
        _paq.push(["setSiteId", pkSiteId]);
        _paq.push(['setCustomVariable', 1, 'PageTime', load_time, 'page']);
        var d = document, g = d.createElement("script"), s = d.getElementsByTagName("script")[0];
        g.type = "text/javascript";
        g.defer = true;
        g.async = true;
        g.src = pkBaseURL + "piwik.js";
        s.parentNode.insertBefore(g, s);
    })();
</script>
<noscript><p><img src="//estatisticas.fnde.gov.br/piwik.php?idsite=20" style="border:0;" alt="" /></p></noscript>
<!-- End Piwik Code -->

</body>
<!-- InstanceEnd -->
</html>
