<script type="text/javascript" language="JavaScript1.2">
function submitForm(){

	 document.formDados.acao.value = "pesquisar";
	 
	if (document.getElementById("planilhas").selectedIndex == 0 ) {
		  document.formDados.descricaoItem.value = document.formDados.planilhas.options[document.formDados.planilhas.selectedIndex].text.replace( /^\s+/g, "" );
		 // document.formDados.nivel.value = document
	}else{
       document.formDados.descricaoItem.value = document.formDados.planilhas.options[document.formDados.planilhas.selectedIndex].text.replace( /^\s+/g, "" );
 	   document.formDados.descricaodoItem.value = document.formDados.planilhas.options[document.formDados.planilhas.selectedIndex-1].text.replace( /^\s+/g, "" );
 	  }
       
       
	 if(document.formDados.planilhas.value != '')
	     document.formDados.submit();
  	 else
		alert("Item Selecionado n\u00E3o possui Planilha de Dados");
	    
}

function atualizarAno(){
	 document.formDados.target= "_self";
	 document.formDados.acao.value = "atualizar";
     document.formDados.periodos.options[document.formDados.periodos.selectedIndex].value = -1;	 
	 document.formDados.submit();
}

function atualizarPeriodo(){
	 document.formDados.target= "_self";
	 document.formDados.acao.value = "atualizar";
	 document.formDados.submit();
}
</script> 



